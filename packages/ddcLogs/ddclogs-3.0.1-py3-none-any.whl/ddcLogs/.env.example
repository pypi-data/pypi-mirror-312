LOG_LEVEL=DEBUG
LOG_UTC=True
LOG_ENCODING=UTF-8
LOG_APP_NAME=app
LOG_FILENAME=app.log
LOG_DIRECTORY=./logs
LOG_DAYS_TO_KEEP=30
LOG_STREAM_HANDLER=True
LOG_SHOW_LOCATION=False
LOG_DATE_FORMAT=%Y-%m-%dT%H:%M:%S

# TimedRotatingLog
LOG_ROTATING_WHEN=midnight
LOG_ROTATING_FILE_SUFIX=%Y%m%d

# SizeRotatingLog
LOG_MAX_FILE_SIZE_MB=10
