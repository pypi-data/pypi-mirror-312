{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "1.2.5"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-st-midasit.com/backend/python-executor/"
    }
  ],
  "paths": {
    "/execute?functionId=moapy-dgnengine-eurocode2_beam-report_ec2_beam": {
      "post": {
        "summary": "Eurocode 2 Beam Design",
        "description": "Eurocode 2 provides a comprehensive review of reinforced concrete (RC) beam design with a focus on strength assessment, available in Excel format.",
        "operationId": "report ec2 beam",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "allOf": [
                  {
                    "$ref": "#/components/schemas/report_ec2_beam"
                  }
                ],
                "title": "Body"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultBytes"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        },
        "x-content-info": {
          "title": "Eurocode 2 Beam Design",
          "description": "Eurocode 2 provides a comprehensive review of reinforced concrete (RC) beam design with a focus on strength assessment, available in Excel format."
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BeamRebarPattern": {
        "properties": {
          "top": {
            "items": {
              "$ref": "#/components/schemas/RebarNumberNameCover"
            },
            "type": "array",
            "title": "Top",
            "description": "Top Rebar"
          },
          "bot": {
            "items": {
              "$ref": "#/components/schemas/RebarNumberNameCover"
            },
            "type": "array",
            "title": "Bot",
            "description": "Bottom Rebar"
          },
          "stirrup": {
            "title": "Stirrup",
            "description": "Stirrup Rebar",
            "$ref": "#/components/schemas/RebarNumberNameSpace"
          }
        },
        "type": "object",
        "title": "Beam Rebar Pattern",
        "dataclassname": "moapy.rc_pre.BeamRebarPattern"
      },
      "Force": {
        "properties": {
          "value": {
            "type": "number",
            "title": "Value",
            "description": "Force"
          },
          "unit": {
            "$ref": "#/components/schemas/enUnitForce"
          }
        },
        "type": "object",
        "title": "Force",
        "description": "Force",
        "dataclassname": "moapy.data_pre.Force"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Length": {
        "properties": {
          "value": {
            "type": "number",
            "title": "Value",
            "description": "Length"
          },
          "unit": {
            "$ref": "#/components/schemas/enUnitLength"
          }
        },
        "type": "object",
        "title": "Length",
        "description": "Length",
        "dataclassname": "moapy.data_pre.Length"
      },
      "MaterialNative": {
        "properties": {
          "fck": {
            "title": "fck",
            "description": "Concrete strength",
            "$ref": "#/components/schemas/Stress"
          },
          "fy": {
            "title": "fy",
            "description": "Rebar strength",
            "$ref": "#/components/schemas/Stress"
          },
          "fys": {
            "title": "fys",
            "description": "Stirrup strength",
            "$ref": "#/components/schemas/Stress"
          }
        },
        "type": "object",
        "title": "MaterialNative",
        "description": "Material Native Data",
        "dataclassname": "moapy.rc_pre.MaterialNative"
      },
      "Moment": {
        "properties": {
          "value": {
            "type": "number",
            "title": "Value",
            "description": "Moment"
          },
          "unit": {
            "$ref": "#/components/schemas/enUnitMoment"
          }
        },
        "type": "object",
        "title": "Moment",
        "description": "Moment",
        "dataclassname": "moapy.data_pre.Moment"
      },
      "RebarNumberNameCover": {
        "properties": {
          "number": {
            "type": "integer",
            "title": "Number",
            "description": "Number of Rebar",
            "default": 2
          },
          "name": {
            "type": "string",
            "enum": [
              "P4",
              "P5",
              "P6",
              "P8",
              "P10",
              "P12",
              "P14",
              "P16",
              "P18",
              "P20",
              "P22",
              "P24",
              "P26",
              "P30",
              "P32",
              "P36",
              "P40"
            ],
            "title": "Name",
            "description": "Rebar Name",
            "default": "P26"
          },
          "cover": {
            "title": "Cover",
            "description": "Distance from centroid of reinforcement to the nearest surface of the concrete",
            "$ref": "#/components/schemas/Length"
          }
        },
        "type": "object",
        "title": "Rebar Number",
        "description": "Rebar Number",
        "dataclassname": "moapy.rc_pre.RebarNumberNameCover"
      },
      "RebarNumberNameSpace": {
        "properties": {
          "number": {
            "type": "integer",
            "title": "Number",
            "description": "Number of legs",
            "default": 2
          },
          "name": {
            "type": "string",
            "enum": [
              "P4",
              "P5",
              "P6",
              "P8",
              "P10",
              "P12",
              "P14",
              "P16",
              "P18",
              "P20",
              "P22",
              "P24",
              "P26",
              "P30",
              "P32",
              "P36",
              "P40"
            ],
            "title": "Name",
            "description": "Rebar Name",
            "default": "P10"
          },
          "space": {
            "title": "Space",
            "description": "Distance between rebars",
            "$ref": "#/components/schemas/Length"
          }
        },
        "type": "object",
        "title": "Rebar Number Name Space",
        "description": "Rebar Number Name Space",
        "dataclassname": "moapy.rc_pre.RebarNumberNameSpace"
      },
      "ResultBytes": {
        "properties": {
          "type": {
            "type": "string",
            "title": "Type",
            "description": "Type",
            "default": "xlsx"
          },
          "result": {
            "type": "string",
            "title": "Result",
            "description": "have to base64 to binary data",
            "default": ""
          }
        },
        "type": "object",
        "title": "Bytes",
        "description": "Result Bytes",
        "dataclassname": "moapy.data_post.ResultBytes"
      },
      "SectionForce": {
        "properties": {
          "Fz": {
            "title": "Fz",
            "description": "Axial force",
            "$ref": "#/components/schemas/Force"
          },
          "Mx": {
            "title": "Mx",
            "description": "Moment about x-axis",
            "$ref": "#/components/schemas/Moment"
          },
          "My": {
            "title": "My",
            "description": "Moment about y-axis",
            "$ref": "#/components/schemas/Moment"
          },
          "Vx": {
            "title": "Vx",
            "description": "Shear about x-axis",
            "$ref": "#/components/schemas/Force"
          },
          "Vy": {
            "title": "Vy",
            "description": "Shear about y-axis",
            "$ref": "#/components/schemas/Force"
          }
        },
        "type": "object",
        "title": "Member Force",
        "description": "Force class\n\nArgs:\n    Fz (Length): Axial force\n    Mx (Length): Moment about x-axis\n    My (Length): Moment about y-axis\n    Vx (Length): Shear about x-axis\n    Vy (Length): Shear about y-axis",
        "dataclassname": "moapy.data_pre.SectionForce"
      },
      "SectionRectangle": {
        "properties": {
          "sectionType": {
            "const": "Solid_Rectangle",
            "title": "Sectiontype",
            "default": "Solid_Rectangle"
          },
          "b": {
            "title": "b",
            "description": "Width",
            "$ref": "#/components/schemas/Length"
          },
          "h": {
            "title": "h",
            "description": "Height",
            "$ref": "#/components/schemas/Length"
          }
        },
        "type": "object",
        "title": "Section Rectangle",
        "description": "Section Rectangle class\n\nArgs:\n    b (Length): Width\n    h (Length): Height",
        "dataclassname": "moapy.data_pre.SectionShapePipe"
      },
      "Stress": {
        "properties": {
          "value": {
            "type": "number",
            "title": "Value",
            "description": "Stress"
          },
          "unit": {
            "$ref": "#/components/schemas/enUnitStress"
          }
        },
        "type": "object",
        "title": "Stress",
        "description": "Stress",
        "dataclassname": "moapy.data_pre.Stress"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "enUnitForce": {
        "type": "string",
        "enum": [
          "N",
          "kN",
          "MN",
          "lbf",
          "kip"
        ],
        "title": "enUnitForce"
      },
      "enUnitLength": {
        "type": "string",
        "enum": [
          "mm",
          "cm",
          "m",
          "in",
          "ft"
        ],
        "title": "enUnitLength"
      },
      "enUnitMoment": {
        "type": "string",
        "enum": [
          "kN.m",
          "kip.in",
          "kip.ft",
          "N.mm",
          "N.m"
        ],
        "title": "enUnitMoment"
      },
      "enUnitStress": {
        "type": "string",
        "enum": [
          "Pa",
          "KPa",
          "MPa",
          "psi",
          "ksi"
        ],
        "title": "enUnitStress"
      },
      "report_ec2_beam": {
        "properties": {
          "matl": {
            "default": {
              "fck": {
                "unit": "MPa",
                "value": 24.0
              },
              "fy": {
                "unit": "MPa",
                "value": 400.0
              },
              "fys": {
                "unit": "MPa",
                "value": 400.0
              }
            },
            "$ref": "#/components/schemas/MaterialNative"
          },
          "sect": {
            "default": {
              "sectionType": "Solid_Rectangle",
              "b": {
                "unit": "mm",
                "value": 400.0
              },
              "h": {
                "unit": "mm",
                "value": 600.0
              }
            },
            "$ref": "#/components/schemas/SectionRectangle"
          },
          "rebar": {
            "default": {
              "top": [
                {
                  "cover": {
                    "unit": "mm",
                    "value": 40.0
                  },
                  "name": "P26",
                  "number": 2
                }
              ],
              "bot": [
                {
                  "cover": {
                    "unit": "mm",
                    "value": 40.0
                  },
                  "name": "P26",
                  "number": 2
                }
              ],
              "stirrup": {
                "name": "P10",
                "number": 2,
                "space": {
                  "unit": "mm",
                  "value": 100.0
                }
              }
            },
            "$ref": "#/components/schemas/BeamRebarPattern"
          },
          "force": {
            "default": {
              "Fz": {
                "unit": "kN",
                "value": 0.0
              },
              "Mx": {
                "unit": "kN.m",
                "value": 0.0
              },
              "My": {
                "unit": "kN.m",
                "value": 0.0
              },
              "Vx": {
                "unit": "kN",
                "value": 0.0
              },
              "Vy": {
                "unit": "kN",
                "value": 0.0
              }
            },
            "$ref": "#/components/schemas/SectionForce"
          }
        },
        "type": "object",
        "title": "report ec2 beam"
      }
    }
  }
}