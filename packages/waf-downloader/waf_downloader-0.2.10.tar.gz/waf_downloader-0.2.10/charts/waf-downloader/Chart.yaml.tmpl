apiVersion: v2
name: waf-downloader
description: A Helm chart for the Cloudflare Web Application Firewall log downloader
type: application
version: "{{ VERSION }}"
appVersion: "{{ VERSION }}"

# Add a maintainers section to provide information about the chart maintainers
maintainers:
  - name: Mihai Bojin
    email: mihai.bojin@gmail.com

# Add annotations to provide additional metadata about the chart
annotations:
  category: Application
  licenses: Apache-2.0

# Add an icon URL if you have one
icon: data:image/svg+xml;base64,

# Add keywords to help with chart discoverability
keywords:
  - waf
  - cloudflare
  - logs
  - downloader
  - firewall

# Add home URL for the project
home: https://MihaiBojin.github.io/waf-downloader/

# Add source repository URL
sources:
  - https://github.com/MihaiBojin/waf-downloader
